package com.review6;

public class deneMe2DArray {

	public static void main(String[] args) {
		int[][] intArray = { 
                { 2, 3, 4 }, 
                { 4, 5, 6 }, 
                { 6, 7, 8 }, 
                { 8, 9, 0 } 
            };
		for (int i=0; i<intArray.length; i++) {
			for (int j=0; j<i; j++) {
				System.out.println(intArray[i][j]);
			}
		}System.out.println();
		
	}

}
