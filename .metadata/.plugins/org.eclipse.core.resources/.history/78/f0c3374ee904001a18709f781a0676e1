package reviewClass;

import java.lang.reflect.Array;

public class BeraberForLoop {

	public static void main(String[] args) {
		
		
		int [][] array= { {3,6,8,9},
				         {67,87,98,56}, 
				         {34,65,55,4},
				         {34,6,22,12},
				         };
		
		for (int i=0; i<array.length; i++) { //row
			for (int k=0; k<array[i].length; k++) {
				
				System.out.print(array[k][i]);
			
			}System.out.println();
			
		}
		
		

	}

}
