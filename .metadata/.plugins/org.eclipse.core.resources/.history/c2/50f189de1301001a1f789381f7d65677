package com.review6;

public class deneMe2DArray {

	public static void main(String[] args) {
		int[][] a = { 
                { 2, 3, 4 }, 
                { 4, 5, 6 }, 
                { 6, 7, 8 }, 
                { 8, 9, 0 } 
            };
		for (int i=0; i<a[i].length; i++) {
			for (int j=0; j<a[i].length; j++) {
				
			}System.out.print(a[i][j]);
		}
		System.out.println();
		
	}

}
