package com.mytrials;

public class TwoDArrays {

	public static void main(String[] args) {
		
		int [][] array= {{1,4,7,4,9},
							{8,11,34,56,23},
							{22,45,76,34,71} };
		
		for (int i=0; i<array.length; i++) {
			for (int j=0; j<array[i].length; j++) {
				System.out.print(array[i][j]+"  ");
				
			}System.out.println();	
		}

	}

}
