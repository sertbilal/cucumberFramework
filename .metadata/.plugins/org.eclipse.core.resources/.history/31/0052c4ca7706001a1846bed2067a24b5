package com.reviewMethodsClass;

public class World {
	public static void main (String[]args) {
		
	Baby baby1=new Baby();
	baby1.firstName="John";
	baby1.lastName="Doe";
	baby1.gender='M';
	baby1.hairColor="Yellow";
	baby1.weight=7;
	
	System.out.println(baby1.firstName);
	
	System.out.println(baby1.gender);
	
	baby1.walk(2);

	
		
		
		
	}
}
